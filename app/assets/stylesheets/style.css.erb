* {
  margin:0;
  padding:0;
}

body {
  font-family:"proxima-nova-1","proxima-nova-2","Helvetica Neue", Helvetica, Arial, sans-serif;
  font-size:18px;
  font-weight:200;
  color:#647185;
  background:url(<%= asset_path "embed/background.png" %>);
}

body.win {font-family:Arial, sans-serif;}

#global {
  width:960px;
  margin:0 auto;
  position:relative;
  top:70px;
  z-index:2;
}

#back {
  width:960px;
  margin:0 auto;
  background:#080d15 url(<%= asset_path "embed/texture.png" %>);
/*  opacity:0.7;*/
  position:absolute;
  left:0;
  right:0;
  top:0;
  bottom:0;
  z-index:2;
  -webkit-box-shadow:#000 0 2px 10px;
  -moz-box-shadow:#000 0 2px 10px;
  box-shadow:#000 0 2px 10px;
  -webkit-border-radius:10px;
  -moz-border-radius:10px;
  border-radius:10px;
}

#logo_backlights {
  width:960px;
  height:292px;
  background:url(<%= asset_path "embed/logo_backlights.png" %>) no-repeat;
  position:absolute;
  margin:0 auto;
  top:-80px;
  left:-220px;
  right:0;
  z-index:1;
}

#main {
  position:relative;
  z-index:3;
}

#header {
  padding:25px;
  position:relative;
}

#header h1 a {
  display:block;
  width:233px;
  height:54px;
  margin:10px 10px 0;
  background:url(<%= asset_path "embed/jilion.png" %>) no-repeat;
}

#header h1 a span, #steve_ribbon span {display:none;}

#menu {
  list-style-type:none;
  position:absolute;
  top:40px;
  right:30px;
  z-index:3;
}

#menu li {
  float:left;
  padding:0 0 0 20px;
  position:relative;
}

#menu li a {
  display:block;
  height:39px;
  padding-right:6px;
  font-size:16px;
  color:#eef2f9;
  text-shadow:#000 0 1px 2px;
  font-weight:600;
  text-transform:uppercase;
  letter-spacing:1px;
  position:relative;
}

#menu li a strong {
  background:url(<%= asset_path "embed/black_btn_left.png" %>) no-repeat;
  height:28px;
  font-weight:600;
  padding:11px 3px 0 12px;
  display:block;
}

#menu li a:active strong {background-position:0 -39px;}

#menu li a span.corner {
  background:url(<%= asset_path "embed/black_btn_right.png" %>) no-repeat;
  display:block;
  width:6px;
  height:39px;
  position:absolute;
  top:0;
  right:0;
}

#menu li a:active span.corner {background-position:0 -39px;}

#menu li.blog a span.icon {
  padding:0 26px 0 0;
  background:url(<%= asset_path "embed/blog_icon.png" %>) no-repeat 100% 0;
}

#menu li.jobs span.note {
  display:block;
  width:82px;
  height:44px;
  background:url(<%= asset_path "embed/jobs_menu_highlight.png" %>) no-repeat 0;
  position:absolute;
  top:-16px;
  left:-66px;
  text-indent:250%;
  white-space:nowrap;
  overflow:hidden;
}

h2.headline {
  text-indent:-99999px;
  display:block;
  width:500px;
  height:36px;
  background:url(<%= asset_path "embed/headline.png" %>) no-repeat;
  margin:9px auto 29px;
}

#latest_work {
  width:910px;
  height:460px;
  margin:0 auto 20px;
  position:relative;
  overflow:hidden;
}

#latest_work .wrap {
  width:1820px;
  position:absolute;
  top:0;
  left:0;
}

#latest_work .box {
  width:910px;
  height:460px;
  font-size:14px;
  font-weight:400;
  text-shadow:#ccc 0 1px 0;
  position:relative;
  float:left;
  -webkit-font-smoothing:antialiased;
}

#latest_work h3, #latest_work h4 {display:none;}

#latest_work .box.framework {
  background:url(<%= asset_path "embed/home/framework.png" %>) no-repeat;
}

#latest_work .box.framework a {
  display:block;
  width:910px;
  height:460px;
}

#latest_work_navigation {
  width:42px;
  height:13px;
  margin:0 auto;
  position:relative;
  top:-12px;
}

#latest_work_navigation a {
  display:block;
  background:url(<%= asset_path "embed/home/navigation_dot.png" %>) no-repeat;
  width:13px;
  height:13px;
  float:left;
  margin:0 4px;
}

#latest_work_navigation a.active {background:url(<%= asset_path "embed/home/navigation_dot_active.png" %>) no-repeat}

#latest_work_navigation a span {display:none;}

#content {
  padding:0 0 16px;
}

#content .section {
  padding:0 0 20px;
  font-size:18px;
}

#content .section h3 {
  padding-bottom:5px;
}

#content .section p {
  line-height:28px;
}

#content .section.press {
  padding-top:10px;
}

#content .section.press ul {
  list-style-type:none;
  padding:0 0 0 30px;
}

#content .section.press ul li {
  float:left;
  height:74px;
  margin:0 20px 0 0;
/*  width:100px;*/
}

#content .section.press ul li a {
  display:block;
}

#content .section.press ul li.readwriteweb a {
  width:135px;
  height:42px;
  margin-top:10px;
  background:url(<%= asset_path "embed/home/logos/readwriteweb.png" %>) no-repeat;
}

#content .section.press ul li.wired a {
  width:117px;
  height:61px;
  margin-top:5px;
  background:url(<%= asset_path "embed/home/logos/wired.png" %>) no-repeat;
}

#content .section.press ul li.daringfireball a {
  width:150px;
  height:74px;
  background:url(<%= asset_path "embed/home/logos/daringfireball.png" %>) no-repeat;
}

#content .section.press ul li.award a {
  width:205px;
  font-size:15px;
  font-weight:600;
  text-align:center;
  line-height:20px;
}

#content .section.press ul li.award a span.star {
  width:26px;
  height:26px;
  display:inline-block;
  margin:0 -3px -7px 0;
  background:url(<%= asset_path "embed/home/award_star.png" %>) no-repeat;
}

#content .section.press ul li span.text {
  display:none;
}

#content .section.main_description {
  padding:0 30px;
  font-size:20px;
}

#content .section.main_description p {
  padding:10px 0;
}

#content .section.news {
  padding:30px 40px 20px;
}

#content .section.news ul.articles {
  list-style-type:none;
}

#content .section.news ul.articles li {
  padding:4px 0 20px 60px;
  background:url(<%= asset_path "embed/home/news_icon.png" %>) no-repeat;
}

#content .section.news ul.articles li h3 {
  font-size:18px;
}

#content .section.news ul.articles li em.date {
  font-size:15px;
  font-style:normal;
  margin-right:10px;
}

#content .section.news ul.articles li a.read {
  font-size:16px;
  padding:0 20px 3px 0;
  background:url(<%= asset_path "embed/link_arrow.png" %>) no-repeat 100% 4px;
}

#content .section.student_project {
  width:420px;
  float:left;
  padding:0 40px;
}

#content .section.box .left {
  float:left;
  width:100px;
  text-align:center;
}

#content .section.student_project .left span.ratings {
  background:url(<%= asset_path "embed/home/aelios/stars.png" %>) no-repeat center top;
  display:block;
  padding:17px 0 20px;
  font-size:12px;
}

#content .section.box .left a.more {
  font-size:16px;
  padding:0 20px 3px 0;
  background:url(<%= asset_path "embed/link_arrow.png" %>) no-repeat 100% 4px;
}

#content .section.box .right {
  float:left;
  width:300px;
  padding:0 0 0 20px;
}

#content .section.box .right h3 {
  font-size:26px;
  padding-bottom:10px;
}

#content .section.open_source {
  width:420px;
  float:left;
}

#content .section.open_source span.downloads {
  font-size:12px;
  display:block;
  padding:6px 0 8px;
}

#content .section.open_source span.downloads a {
  font-size:15px;
  display:block;
}

#content .section.break {
  width:910px;
  height:2px;
  margin:30px auto 15px;
  background:url(<%= asset_path "embed/home/break.png" %>) no-repeat center;
}

#content .section.people {
  line-height:28px;
  padding:0 40px;
}

#content .section.people ul.photos {
  width:432px;
  padding:16px 0 0;
  margin:0 auto 16px;
  list-style-type:none;
  line-height:0;
}

#content .section.people ul.photos li {
  width:56px;
  height:56px;
  float:left;
  display:block;
  margin:0 8px 15px;
  position:relative;
  background:#000;
}

#content .section.people ul.photos li .details_box {
  display:none;
  position:absolute;
  top:-42px;
  left:-38px;
  z-index:5;
  width:120px;
  padding:6px;
  background:#272f3a;
  -moz-border-radius:6px;
  -webkit-border-radius:6px;
  border-radius:6px;
  -moz-box-shadow:inset rgba(172,204,255,0.2) 0 1px 0, #000 0 1px 14px, #000 0 1px 14px;
  -webkit-box-shadow:inset rgba(172,204,255,0.2) 0 1px 0, #000 0 1px 14px, #000 0 1px 14px;
  box-shadow:inset rgba(172,204,255,0.2) 0 1px 0, #000 0 1px 14px, #000 0 1px 14px;
}

@media screen and (-webkit-min-device-pixel-ratio:0) {
  #content .section.people ul.photos li .details_box {
    display:block;
    -webkit-transition: opacity .15s ease-out, -webkit-transform .3s ease-out;
    -webkit-transform: scale3d(0.46,0.46,0.46) translate3d(0,-10px,0);
    opacity:0;
    -webkit-transform-origin:center;
  }

  #content .section.people ul.photos li .details_box.double_line {
    -webkit-transform: scale3d(0.46,0.46,0.46) translate3d(0,-23px,0);
  }

  #content .section.people ul.photos li:hover .details_box {
    -webkit-transform: scale3d(1,1,1) translate3d(0,0,0);
    opacity:1;
  }
}

#content .section.people ul.photos li:hover .details_box {display: block}

#content .section.people ul.photos li .details_box h4 {
  display:block;
  color:#acbbd4;
  font-size:15px;
  line-height:20px;
  font-weight:500;
  text-align:center;
  padding:10px 0 4px;
  text-shadow:rgba(0,0,0,0.4) 0 -1px 0;
}

#content .section.people p {
  padding:16px 0 40px;
  width:600px;
  margin:0 auto;
  text-align:center;
}

#content .section.contact {
  width:516px;
  height:98px;
  margin:0 auto;
  background:url(<%= asset_path "embed/contact_back.png" %>) no-repeat;
  position:relative;
}

#content .section.contact h3 {
  position:relative;
  top:13px;
  left:20px;
  text-shadow:0 -1px 0 #0b0c0f;
}

#content .section.contact ul {
  list-style-type:none;
  position:absolute;
  bottom:40px;
  left:4px;
}

#content .section.contact li {
  float:left;
  padding:0 0 0 16px;
}

#content .section.contact li a {
  display:block;
  height:33px;
  padding-right:6px;
  font-size:12px;
  color:#eef2f9;
  font-weight:600;
  text-shadow:#000 0 1px 1px;
  position:relative;
}

#content .section.contact li a strong {
  background:url(<%= asset_path "embed/black_btn_small_left.png" %>) no-repeat;
  height:24px;
  font-weight:600;
  padding:9px 4px 0 8px;
  display:block;
}

#content .section.contact li a:active strong {background-position:0 -33px;}

#content .section.contact li a span.corner {
  background:url(<%= asset_path "embed/black_btn_small_right.png" %>) no-repeat;
  display:block;
  width:6px;
  height:33px;
  position:absolute;
  top:0;
  right:0;
}

#content .section.contact li a:active span.corner {background-position:0 -33px;}

#content .section.contact li.email a span.icon {
  padding:0 0 5px 28px;
  background:url(<%= asset_path "embed/email_icon.png" %>) no-repeat 0 0;
}

#content .section.contact li.map a span.icon {
  padding:2px 0 5px 20px;
  background:url(<%= asset_path "embed/map_icon.png" %>) no-repeat 0 0;
}

#content .section.contact li.twitter a span.icon {
  padding:2px 0 5px 32px;
  background:url(<%= asset_path "embed/twitter_icon.png" %>) no-repeat 0 0;
}

#content .section.contact .newsletter {
  width:180px;
  position:absolute;
  bottom:40px;
  right:20px;
}

#content .section.contact .newsletter label {
  font-size:12px;
  font-weight:500;
  color:#818694;
  text-shadow:rgba(0,0,0,0.9) 0 -1px 0;
  padding-bottom:4px;
  display:block;
}

#content .section.contact .newsletter .wrap {
  background:url(<%= asset_path "embed/input_back.png" %>) no-repeat 0 0;
  width:180px;
  display:block;
  position:relative;
  height:33px;
  left:0;
  top:0;
}

#content .section.contact .newsletter input.text {
  border:none;
  background:none;
  font-size:13px;
  padding:4px 4px 4px 5px;
  width:140px;
  margin-top:4px;
  display:inline;
  outline:0;
}

#content .section.contact .newsletter button.submit {
  width:32px;
  height:32px;
  background:url(<%= asset_path "embed/submit_arrow_btn.png" %>) no-repeat;
  border:none;
  position:absolute;
  right:0;
  top:0;
  outline:0;
}

#content .section.contact .newsletter button.submit:active {background-position:0 -32px;}

#content .section.contact .newsletter button.submit span {display:none;}

#email_placeholder {
  position:absolute;
  top:9px;
  left:10px;
  text-shadow:none;
  font-weight:600;
  font-size:12px;
  color:#2f3849;
  text-shadow:#b7bdc5 0 1px 0;
}

h3.title {
  font-size:20px;
  letter-spacing:1px;
  text-transform:uppercase;
  font-weight:600;
}

h3.title.latest_work {
  color:#cfdcf3;
  text-shadow:#000 0 -1px 0;
  padding:18px 0 0 20px;
  font-weight:600;
}

h3.title.news, .what_we_do span.highlight {color:#3bbbf0;}
h3.title.student_project {color:#a944f4;}
h3.title.people, .people span.highlight {color:#f14363;}
h3.title.open_source {color:#ef6641;}
h3.title.contact {color:#a7b1c1;}

@media screen and (-webkit-min-device-pixel-ratio:0){
  h3.title {
    position:relative;
    text-shadow:0 0 1px #000;
    height:35px;
    width:200px;
  }

  h3.title span {
    position:absolute;
    top:0;
    left:0;
    z-index:2;
    -webkit-mask-image: -webkit-gradient(linear, left top, left bottom, from(rgba(0,0,0,1)), to(rgba(0,0,0,0)));
  }

  h3.title.news:after {content:'Latest news';}
  h3.title.news {color:#0097e3;}
  h3.title.news span {color:#6bcbf9;}

  h3.title.student_project:after {content:'Student project';}
  h3.title.student_project {color:#530090;}
  h3.title.student_project span {color:#cc86ff;}

  h3.title.people:after {content:'People';}
  h3.title.people {height:25px;color:#d9002f;}
  h3.title.people span {color:#ff4c6e;}

  h3.title.open_source:after {content:'Open-source';}
  h3.title.open_source {color:#e14400;}
  h3.title.open_source span {color:#fc8247;}

  h3.title.contact:after {content:'Get in touch';}
  h3.title.contact {height:25px;color:#737a85;}
  h3.title.contact span {color:#bfc9d8;-webkit-mask-image: -webkit-gradient(linear, left top, left bottom, from(rgba(0,0,0,0)), to(rgba(0,0,0,1)));}
  h3.title.contact.alt span {color:#bfc9d8;-webkit-mask-image: -webkit-gradient(linear, left top, left bottom, from(rgba(0,0,0,1)), to(rgba(0,0,0,0)));}

}

#footer {
  width:840px;
  height:88px;
  background:url(<%= asset_path "embed/footer_back.png" %>) no-repeat center top;
  padding:60px;
  margin:0 auto;
  text-align:center;
  font-size:12px;
  font-weight:400;
  text-transform:uppercase;
  color:#07090e;
  text-shadow:#4E5771 0 1px 0;
  position:relative;
  z-index:1;
  top:30px;
}

.spacer {clear:both;}

.hw_enabler {
  width:0;
  height:0;
  -webkit-animation:sv_hw_enable 5s infinite linear;
}

@-webkit-keyframes sv_hw_enable {from{-webkit-transform:rotate(0deg);}to{-webkit-transform:rotate(360deg)}}

a {
  text-decoration:none;
  color:#d6e0f1;
  font-weight:400;
  outline:0;
  -webkit-transition:color 0.5s;
}

a:hover {
  color:#fff;
}

button::-moz-focus-inner { border: 0; }

::selection {
  background:rgba(24,127,247,.2);
  color:#fff;
  text-shadow:none;
}

::-moz-selection {
  background:#ffb7b7;
}

#email_flash {
  background:#92c9ad url(<%= asset_path "embed/home/valid.png" %>) no-repeat 5px 7px;
  float:right;
  height:24px;
  padding:8px 7px 0 24px;
  text-shadow:none;
  color:#016130;
  font-weight:bold;
  font-size:12px;
  -webkit-border-radius:6px;
  -moz-border-radius:6px;
  border-radius:6px;
}

#email_flash.thankyou {
  background:#c9b77c url(<%= asset_path "embed/home/valid_orange.png" %>) no-repeat 5px 7px;
  color:#614a01;
}

#map_background {
  position:fixed;
  width:100%;
  height:100%;
  top:0;
  left:0;
  right:0;
  bottom:0;
  background:#000;
  opacity:0.6;
  z-index:100;
}

#map_overlay {
  position:absolute;
  top:85px;
  left:50%;
  margin-left:-465px;
  width:910px;
  height:500px;
  z-index:101;
  border:10px solid #000;
  -webkit-box-shadow:rgb(0,0,0) 0 2px 10px;
  -moz-box-shadow:rgb(0,0,0) 0 2px 10px;
  box-shadow:rgb(0,0,0) 0 2px 10px;
  -webkit-border-radius:8px;
  -moz-border-radius:8px;
  border-radius:8px;
}

#map_wrap {
  width:910px;
  height:500px;
}

#popbox {
  font-size:15px;
  line-height:25px;
}

#popbox a {
  font-weight:200;
  color:#8b929f;
}

#contact {
  padding:20px 45px 35px;
  position:relative;
  color:#fff;
}

ul#contact_topics {
  list-style-type:none;
}

ul#contact_topics > li {
  margin:10px 0;
  background-color:rgba(0,0,0,0.7);
  position:relative;
  -webkit-border-radius:10px;
  -moz-border-radius:10px;
  border-radius:10px;
}


ul#contact_topics > li.expanded, ul#contact_topics li a.title:hover {background-color:#400038;}

ul#contact_topics li a.title {
  font-size:19px;
  font-weight:100;
  padding:20px;
  display:block;
  position:relative;
  -webkit-border-radius:10px;
  -moz-border-radius:10px;
  border-radius:10px;
  -webkit-transition-property: background-color, color;
  -webkit-transition-duration: 1s, 1s;
}

ul#contact_topics li a.title strong {
  font-weight:600;
  color:#c9d5e3;
  -webkit-transition: color 1s;
}

ul#contact_topics li a.title:hover strong, ul#contact_topics li.expanded a.title strong {
  color:#fff;
}

ul#contact_topics li.expanded a.title, ul#contact_topics li.expanded a.title:hover {
  color:#fff;
  background-color:rgba(0,0,0,0.3);
  -webkit-border-bottom-left-radius:0;
  -webkit-border-bottom-right-radius:0;
  -moz-border-radius:6px 6px 0 0;
  border-radius:6px 6px 0 0;
}

ul#contact_topics li a.title span.arrow {
  display:none;
  width:21px;
  height:21px;
  background:url(<%= asset_path "embed/contact/arrow.png" %>) no-repeat;
  position:absolute;
  top:20px;
  right:20px;
}

ul#contact_topics li.expanded a.title span.arrow {
  -webkit-transform:rotate(90deg);
}

ul#contact_topics .form_box {
  display:none;
  padding:0 20px;
  width:600px;
}

ul#contact_topics .form_box h2 {display:none;}

ul#contact_topics .form_box .intro {
  font-size:16px;
  padding:15px 0 0;
}

ul#contact_topics .form_box .intro p {
  line-height:26px;
  padding:4px 0;
}

ul#contact_topics .form_box .intro p:last-child {
  padding-bottom:0;
}

ul#contact_topics .form_box .intro ul {
  line-height:26px;
  list-style-type:none;
  padding:0 0 10px;
}

ul#contact_topics .form_box .intro ul li {
  padding:3px 24px;
  background:url(<%= asset_path "embed/contact/li.png" %>) no-repeat 0 11px;
}

ul#contact_topics .form_box .intro a {
  color:#fff;
  border-bottom: 2px solid rgba(255,255,255,0.2);
}

#contact .required_mention {
  position:absolute;
  bottom:20px;
  right:20px;
  font-size:14px;
  font-weight:500;
}

#contact form {
/*  width:600px;*/
}

#contact form h4 {
  font-weight:300;
  font-size:26px;
  padding:40px 0 0;
}

#contact form label {
  display:block;
  font-size:14px;
  font-weight:500;
/*  color:#7691b3;*/
  padding:0 0 3px;
  margin:22px 0 0;
  color:#fff;
}

#contact form label.radio {
  margin:10px 0 0;
  line-height:18px;
}

#contact form label.select {
  margin:0;
}

#contact label em {
  display:block;
  font-weight:100;
  font-size:14px;
  color:rgba(255,255,255,0.5);
}

#contact form label abbr, #contact .required_mention abbr {
  color:#ce0038;
  font-style:normal;
  font-size:20px;
  border:none;
}

#contact form input.text {
  width:99%;
  font-size:16px;
  padding:5px 2px;
  border:none;
}

#contact form .textarea_wrapper {line-height:12px;}

#contact form textarea {
  width:99%;
  height:200px;
  padding:3px 2px;
  font-size:14px;
  border:none;
  resize:vertical;
}

#contact form input.text, #contact form textarea {
  background:#ffd3f9;
  border:none;
  font-family:Courier,Monaco,monospace;
  font-size:13px;
  -webkit-transition:0.5s background-color;
  -webkit-border-radius:4px;
}

#contact form .field_with_errors input.text, #contact form .field_with_errors textarea {
  -webkit-border-bottom-left-radius:0;
  -webkit-border-bottom-right-radius:0;
  -moz-border-radius:6px 6px 0 0;
  border-radius:6px 6px 0 0;
}

#contact form input.text:focus, #contact form textarea:focus {
  background:#fff;
  outline-color:#ff70ed;
}

#contact form .inline_errors {
  background:#e06300;
  width:582px;
  padding:4px 8px;
  color:#fff;
  font-size:13px;
  font-weight:400;
  display:block;
  -webkit-border-bottom-left-radius:6px;
  -webkit-border-bottom-right-radius:6px;
  -moz-border-radius:0 0 6px 6px;
  border-radius:0 0 6px 6px;
}

#contact form .textarea_wrapper .inline_errors {
  padding:6px 8px;
}

#contact form .file .inline_errors {
  display:inline;
  -moz-border-radius: 6px;
  -webkit-border-radius: 6px;
  border-radius: 6px;
}

#contact form label.radio input[type=radio] {
  float:left;
  margin:2px 6px 0 0;
}

#contact form label.radio .inline_errors {
  display:inline;
/*  margin:0 0 3px 5px;*/
  padding:2px 6px;
  -webkit-border-radius:6px;
  -moz-border-radius:6px;
  border-radius:6px;
}

#contact form .entry.file {
  position:relative;
}

#contact form .entry.file .fake_file_input {
  height:28px;
  position:relative;
}

#contact form .entry.file .fake_file_input_button {
  width:115px;
  height:28px;
  background:url(<%= asset_path "embed/contact/file_button_back.png" %>) no-repeat;
  -webkit-border-radius:6px;
  -moz-border-radius:6px;
  border-radius:6px;
}

#contact form .entry.file .fake_file_input_button.active {background-position:0 -28px;}

#contact form .entry.file .fake_file_input_button span {
  display:block;
  padding:6px 8px;
  font-size:14px;
  font-weight:400;
  text-align:center;
  text-shadow:#232932 0 -1px 0;
  background:url(<%= asset_path "embed/contact/file_icon.png" %>) no-repeat 8px 6px;
}

#contact form .entry.file .fake_file_input_value {
  width:400px;
  height:28px;
  position:absolute;
  top:6px;
  left:122px;
  font-size:14px;
}

#contact form .entry.file input {
  opacity:0;
  -moz-opacity:0;
  filter:alpha(opacity=0);
  position:absolute;
  top:20px;
  left:0;
  width:115px;
  height:28px;
  z-index:2;
}

#contact .entry.submit {
  padding:30px 0 20px;
}

#contact .entry.submit input {
  width:115px;
  height:39px;
  background:url(<%= asset_path "embed/contact/button.png" %>) no-repeat;
  border:none;
  outline:0;
  font-family:"proxima-nova-1","proxima-nova-2","Helvetica Neue", Helvetica, Arial, sans-serif;
  font-size:18px;
  font-weight:600;
  color:#302131;
  text-shadow:#9a899a 0 1px 0;
}

#contact .entry.submit input:active {background-position:0 -39px;}

#contact .entry.submit input {float:left;margin-right:10px;}

#contact .entry.submit p.info {
  padding:4px 0 0 0;
  font-size:15px;
  color:rgba(255,255,255,0.6);
}

#contact .entry.submit .spinner {
  float:left;
  padding:4px 10px 0 0;
}

#contact #flash.notice {
  position:absolute;
  top:55px;
  left:45px;
  width:870px;
  height:274px;
  background:rgba(0,0,0,0.9);
  -webkit-border-radius:10px;
  -moz-border-radius:10px;
  border-radius:10px;
}

#contact #flash.notice p {
  text-align:center;
  height:274px;
  line-height:274px;
  font-size:28px;
}

#contact #flash.notice.attachment p {
  line-height:370px;
}

#contact #flash.notice a.pdf {
  display:block;
  width:270px;
  height:55px;
  line-height:55px;
  background:url(<%= asset_path "embed/contact/pdf_icon.png" %>) no-repeat;
  margin:0 auto;
  position:absolute;
  bottom:140px;
  left:0;
  right:0;
  text-align:right;
  font-size:20px;
}

#contact #flash.notice a.remove {
  display:block;
  width:14px;
  height:14px;
  background:url(<%= asset_path "embed/contact/remove.png" %>) no-repeat;
  position:absolute;
  top:20px;
  right:20px;
}

#contact #flash.notice a.remove span {display:none;}

